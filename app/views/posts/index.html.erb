<div class="main">
  <div class="container">
    Click Item to see details
    <% @posts.each do |post| %>
      <div class="posts-index-post">
        <%= link_to("#{post.user} #{post.location} $", post) %>
      </div>
    <% end %>
    <%= link_to 'Post New Item', new_post_path %>
  </div>
</div>